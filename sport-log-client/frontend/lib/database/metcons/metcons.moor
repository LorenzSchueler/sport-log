
import 'metcon_tables.dart';

metconHasMetconSession: SELECT 1 FROM metcon_sessions
    WHERE metcon_id == :id AND deleted == false;

_metconExists: SELECT 1 FROM metcons
    WHERE id == :id AND deleted == false;

_idsOfMetconMovementsOfMetcon: SELECT id from metcon_movements
    WHERE metcon_id == :id AND deleted == false;

_metconMovementWithMovementExists: SELECT 1 FROM metcon_movements
    WHERE movement_id == :id AND deleted == false;